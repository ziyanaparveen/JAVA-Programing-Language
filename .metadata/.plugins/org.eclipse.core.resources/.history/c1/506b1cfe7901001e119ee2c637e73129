
public class B extends A {
	
	
	int a;
	
	B(){
		
		
		System.out.println("its a B constructor");
	}
	
	B(int a){
		
		System.out.println("its an argument constructor of B");
		
	}
	void display() {
		
		System.out.println("its B");
	}
	
	
    void basedisplay() {
	
	
	super.display();
}

      void result() {
    	  
    	  a=20;
    	  super.a=30;
    	 int c=a+super.a;
    	 System.out.println(c);
      }
      
      public static void main(String[] args) {
    	  
    	  B b=new B(10);
      }
		
	}
      


