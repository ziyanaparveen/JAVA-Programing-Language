
public class B extends A {
	
	
	int a;
	
	void display() {
		
		System.out.println("its B");
	}
	public static void main(String[] args) {
		
	}
	
	B(){
		
		
		System.out.println("its a B constructor");
	}
	
	B(int a){
		super(10);
		
		System.out.println("its an argument constructor of B");
		
	}
	void display() {
		
		System.out.println("its B");
	}
	
	
void display() {
	super.display();
}
}
